{
  "name": "dragselect",
  "version": "2.0.0",
  "description": "easy javascript drag select functionality for your projects",
  "main": "./dist/DragSelect.js",
  "dependencies": {},
  "scripts": {
    "prestart": "npm i",
    "start": "npm run open & npm run watch",
    "open": "open __tests__/quicktest.html -a 'Google Chrome'",
    "watch": "concurrently --kill-others \"npm run checkjs:watch\" \"npm run rollup:watch\" \"npm run test:watch\"",
    "checkjs": "tsc --lib es6,dom --allowJs --checkJs --noEmit src/*.js",
    "checkjs:watch": "tsc -w --lib es6,dom --allowJs --checkJs --noEmit src/*.js",
    "build": "npm run rollup && npm run test && npm run media && npm run docs",
    "prerollup": "npm run au",
    "rollup": "rollup -c",
    "rollup:watch": "npm run rollup -- -w",
    "premedia": "rm -r docs/media &",
    "media": "cp -R .media docs/media",
    "au": ".bin/assume-unchanged.sh",
    "docs": "jsdoc -c jsdoc.json",
    "test": "npm run checkjs && jest --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/compat-data": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.8.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "better-docs": "^2.3.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "jsdoc": "^3.6.6",
    "minami": "^1.2.3",
    "prettier": "^2.2.0",
    "puppeteer": "^5.5.0",
    "rollup": "^2.33.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThibaultJanBeyer/DragSelect.git"
  },
  "keywords": [
    "drag",
    "select",
    "plain",
    "js",
    "javascript",
    "vanillajs"
  ],
  "author": "Thibault Jan Beyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThibaultJanBeyer/DragSelect/issues"
  },
  "homepage": "https://ThibaultJanBeyer.com/"
}
